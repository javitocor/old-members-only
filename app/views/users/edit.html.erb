<% provide(:title, 'Edit' %>
<h1>Edit <%= @user.username %></h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= render 'form' %>
    </div>
    <%= link_to '<<Back to user profile', @user %> </br>
    <%= link_to 'Delete', user_path(@user), method: :delete, data: { confirm: "Are you sure?" } %>
</div>
